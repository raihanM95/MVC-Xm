
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="form-horizontal">

    <div class="form-group">
        <label for="" class="control-label col-md-2">District</label>
        <div class="col-md-10">
            @Html.DropDownList("District", null, new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group">
        <label for="" class="control-label col-md-2">Students</label>
        <div class="col-md-10">
            <select id="Student" name="Student" class="form-control">
                <option value="">Select...</option>
            </select>
        </div>
    </div>
    <div class="form-group" id="divStudentInfo">

    </div>
</div>

@section Scripts{

    <script src="~/Scripts/App/StudentDetails.js"></script>
}
